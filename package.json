{
    "name": "@ludeschersoftware/json-object-ref",
    "version": "1.0.0",
    "description": "Resolve absolute and relative references inside JSON-like objects",
    "publishConfig": {
        "access": "public"
    },
    "main": "index.js",
    "type": "commonjs",
    "keywords": [
        "json",
        "ref",
        "reference",
        "config",
        "resolver"
    ],
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "watch": "tsc --watch",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "types": "index.ts",
    "exports": {
        ".": {
            "import": "./index.js",
            "types": "./index.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ludeschersoftware/JJsonObjectRef.git"
    },
    "author": "Johannes Ludescher",
    "license": "MIT",
    "devDependencies": {
        "@types/istanbul-lib-coverage": "^2.0.6",
        "@types/istanbul-lib-report": "^3.0.3",
        "@types/istanbul-reports": "^3.0.4",
        "@types/jest": "^30.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.0.4",
        "jsdom": "^26.1.0",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}